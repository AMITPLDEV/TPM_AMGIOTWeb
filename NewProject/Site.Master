                $("#mainBody").addClass("light-mode");
                localStorage.setItem("selectedTheme", "lightTheme");
                //  $(menu).css("border-bottom", "3px solid #00669f");
                //$(menu).css("background-color", "unset");
                //$(menu).css("color", "#000000ab");
                // $(menu).addClass("aa");
