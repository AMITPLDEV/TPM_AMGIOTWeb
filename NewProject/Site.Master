            if ($('#' + panelid).css('visibility') == "hidden") {
                document.getElementById(panelid).style.visibility = "visible";
                $("#" + panelid).css({ 'right': rightvalue, 'top': topvalue });
            } else {
                document.getElementById(panelid).style.visibility = "hidden";
            }
