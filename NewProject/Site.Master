        function setSideBar() {
            var pathname = window.location.pathname;
            var res = pathname.split("/");
            if (res.length > 2) {
                res = res[2].split(".");
            } else {
                res = res[1].split(".");
            }
            res = res[0].toLowerCase();

            console.log("Resssssss" + res);

            if ((res == "packagingandexportinvoice") || (res == "podetailsmaster") || (res == "documentsdetails")) {
                $("#masters").css("display", "none");
                $('#transactions').css("display", "block");

            }
            else if ((res == "parameterdropdownlistmaster") || (res == "parameterdetailsmaster") || (res == "employeeinformation") || (res == "roleaccessrights") || (res == "customerdetails") || (res == "parameterdropdownlistmaster")) {
                $("#masters").css("display", "block");
                $('#transactions').css("display", "none");
            }
            else {
                $("#masters").css("display", "none");
                $('#transactions').css("display", "none");
            }
        }


