    <script type="text/javascript">
        if (localStorage.getItem("selectedTheme")) {
            if (localStorage.getItem("selectedTheme") == "lightTheme") {
                $("#mainBody").addClass("light-mode");
                $("#toogleTheme").prop('checked', true);
            } else {
                $("#mainBody").css('transition', 'all 1s ease');
                $("#mainBody").removeClass("light-mode");
                $("#toogleTheme").prop('checked', false);
            }
        }
       
        if (localStorage.getItem("selectedMenu")) {
            //$("#" + localStorage.getItem("selectedMenu")).addClass("menu-selected");
            $('#menuddl').text("");
            $('#menuddl').text(localStorage.getItem("selectedMenu"));
            //  alert(" ss = "+ localStorage.getItem("selectedMenu"));
        } else {
             $.ajax({
                async: false,
                type: "POST",
                url: '<%= ResolveUrl("Login.aspx/checkSuperAdminOrNot") %>',
                contentType: "application/json; charset=utf-8",
                crossDomain: true,
              //  data: '{}',
                dataType: "json",
                success: function (response) {
                    let dataitem = response.d;
                    if (dataitem) {
                          $('#menuddl').text("Masters");
                    } else {
                         $('#menuddl').text("Production");
                    }
                },
                error: function (Result) {
                    alert("Error" + Result);
                }
            });
            
        }
        if (localStorage.getItem("selectedMenuID")) {
            let selectmenuId = localStorage.getItem("selectedMenuID");
            $(".ul-sub-menu").css('display', 'none');
            if (selectmenuId == "liProduction") {
                $("#ulProduction").css('display', 'block');
            }
            if (selectmenuId == "liProcessParameter") {
                $("#ulProcessParameter").css('display', 'block');
            }
            if (selectmenuId == "liMasters") {
                $("#ulMasters").css('display', 'block');
            }
            if (selectmenuId == "liQuality") {
                $("#ulQuality").css('display', 'block');
            }
            if (selectmenuId == "liMaintenance") {
                $("#ulMaintenance").css('display', 'block');
            }
            if (selectmenuId == "liEnergy") {
                $("#ulEnergy").css('display', 'block');
            }
            if (selectmenuId == "liAssets") {
                $("#ulAssets").css('display', 'block');
            }

        } else {

             $.ajax({
                async: false,
                type: "POST",
                url: '<%= ResolveUrl("Login.aspx/checkSuperAdminOrNot") %>',
                contentType: "application/json; charset=utf-8",
                crossDomain: true,
              //  data: '{}',
                dataType: "json",
                success: function (response) {
                    let dataitem = response.d;
                    if (dataitem) {
                          $("#ulMasters").css('display', 'block');
                    } else {
                          $("#ulProduction").css('display', 'block');
                    }
                },
                error: function (Result) {
                    alert("Error" + Result);
                }
            });
          
        }
        if (localStorage.getItem("selectedSubMenu")) {
            if ($("#mainBody").hasClass("light-mode")) {
                // $(".menuData").removeClass("selected-menu-style menu0-select").addClass("other-menu-style");
                //$(".menuData").closest('li').find('i').removeClass();
                $("#" + localStorage.getItem("selectedSubMenu")).addClass("selected-menu-style");
                $("#" + localStorage.getItem("selectedSubMenu")).closest('li').find('i').addClass("arrow up");
            } else {
                // $(".menuData").removeClass("selected-menu-style menu0-select").addClass("other-menu-style");
                //$(".menuData").closest('li').find('i').removeClass();
                $("#" + localStorage.getItem("selectedSubMenu")).addClass("selected-menu-style");
                $("#" + localStorage.getItem("selectedSubMenu")).closest('li').find('i').addClass("arrow up");
            }
        } else {

              $.ajax({
                async: false,
                type: "POST",
                url: '<%= ResolveUrl("Login.aspx/checkSuperAdminOrNot") %>',
                contentType: "application/json; charset=utf-8",
                crossDomain: true,
