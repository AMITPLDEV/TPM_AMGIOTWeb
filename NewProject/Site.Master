        function showSubItems(element, ulid) {
            if ($('#' + ulid).css('display') == "none") {
                $('#' + ulid).css({ 'display': 'block', 'border-top': '1px solid #dfdfdfcf', 'border-bottom': '1px solid #dfdfdfcf' });
                $('#' + ulid).closest('div').find('#downuparrow').removeClass();
                $('#' + ulid).closest('div').find('#downuparrow').addClass('glyphicon glyphicon-menu-up');
            } else {
                $('#' + ulid).css('display', 'none');
                $('#' + ulid).closest('div').find('#downuparrow').removeClass();
                $('#' + ulid).closest('div').find('#downuparrow').addClass('glyphicon glyphicon-menu-down');
            }
        }

