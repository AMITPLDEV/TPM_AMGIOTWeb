                projectTheme = "light";
            } else {
                $("#mainBody").css('transition', 'all 1s ease');
                $("#mainBody").removeClass("light-mode");
                localStorage.setItem("selectedTheme", "darkTheme");
                projectTheme = "dark";
            }
            $.ajax({
                async: false,
                type: "POST",
                url: '<%= ResolveUrl("Troubleshooting.aspx/setThemeInSession") %>',
